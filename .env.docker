# Docker Compose environment variables
# Copy this to .env and fill in your values before running docker compose up

# Required: Groq API key
SFA_GROQ_API_KEY=your_groq_api_key_here
SFA_GROQ_MODEL=llama-3.3-70b-versatile

# Optional: GitHub remediation
SFA_GITHUB_TOKEN=
SFA_GITHUB_ROLLBACK_REPO=
SFA_CONFIDENCE_THRESHOLD_FOR_ACTION=0.7